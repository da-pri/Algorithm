#include<stdio.h>
 
int main() {
    int t, m, s, d;
    scanf("%d %d %d", &t, &m, &s);
    scanf("%d", &d);
    t += d / (60 * 60);
    // 24시간 오버시
    m += d % (60 * 60) / 60;
    s += d % (60 * 60) % 60;
 
    if (s >= 60) {
        s -= 60;
        m++;
    }
 
    if (m >= 60) {
        m -= 60;
        t++;
    }
 
    if (t >= 24)
        t %= 24;
 
    printf("%d %d %d", t, m, s);
    return 0;
}
